{
  "canisters": {
    "hello_world_backend": {
      "candid": "hello_world_backend/hello_world_backend.did",
      "package": "hello_world_backend",
      "type": "rust"
    },
    "bloc_ledger":{
      "main": "bloc_ledger/main.mo",
      "type": "motoko"
    },
    "bloc":{
      "main": "bloc/main.mo",
      "type": "motoko",
      "dependencies": ["hello_world_backend"]
    },
    "assets": {
      "dependencies": ["hello_world_backend"],
      "frontend": {
        "entrypoint": "dist/index.html"
      },
      "source": ["dist/"],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "ic": {
      "providers": ["https://mainnet.dfinity.network"],
      "type": "persistent"
    }
  },
  "version": 1
}
